<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mbc.koh10.member.MemberService">

 <select id="idcheck" resultType="int">
 select count(*) from member0402 where id=#{param1}
 </select>
    
<insert id="inserts">
insert into member0402 values(#{param1},#{param2},#{param3})
</insert>

<select id="allout">
select * from member0402
</select>
</mapper>

